<?xml version="1.0" encoding="utf-8"?>
<tvt>
	<resources>
		<data name="DEV_CONFIG">
			<DEV_KEYS value="TRUE" />
			<DEV_OSD value="FALSE" />
			<DEV_LOG value="TRUE" />
			<!-- a modifier cutting the used factor to calculate the
			     revenue of a infomercial: 0.5 would cut the result to 50%
			     Default: 1.0
				<DEV_INFOMERCIALCUTFACTOR value="1.0" />
			-->

			<!-- milliseconds waiting on a floor (incl. animation).
			     Default: 1500
				<DEV_ELEVATOR_WAITTIME value="1500" />
			-->
			<!-- millseconds/frame (open/close animation, 8 frames each).
			     Default: 60
				<DEV_ELEVATOR_ANIMSPEED value="60" />
			-->
			<!-- speed in pixels per second.
			     Default: 130
				<DEV_ELEVATOR_SPEED value="140" />
			-->

			<DEV_DAILY_BOSS_VISIT value="1" />

			<!-- Which kind of contract sort is used in the adagency
			     Options:
			       "minaudience" - by minimum audience required
			       "classification" - by channel-groups (worst, avg, best) and daytime/primetime
			       "profit" - by profit of the contract
			     Default: "minaudience"
				<DEV_ADAGENCY_SORT_CONTRACTS_BY value="minaudience" />
			-->

			<!-- How many contracts could use the same contract base
			     (the same "original contract") at the same time
			     Options:
			       0 - disable any limit
			       x - allow up to x contract instances 
			     Default: 1
				<DEV_ADAGENCY_LIMIT_CONTRACT_INSTANCES value="1" />
			-->

			<!-- (base) construction time of a new bought station
			     time in hours
			     Default: 0 (ready on next broadcast)

				<DEV_STATION_CONSTRUCTION_TIME value="0" />
			-->

			<!-- profit/penalty get multiplied with this value if there
			     is a limiting targetgroup.
			     Default: 1.0
				<DEV_AD_LIMITED_TARGETGROUP_MULTIPLIER value="1.0" />
			-->
			<!-- profit/penalty get multiplied with this value if there
			     is a limiting genre defined.
			     Default: 1.5
				<DEV_AD_LIMITED_GENRE_MULTIPLIER value="1.5" />
			-->
			<!-- profit/penalty get multiplied with this value if there
			     is a limiting flag defined (just xrated, bmovies...).
			     Default: 1.25
				<DEV_AD_LIMITED_PROGRAMME_FLAG_MULTIPLIER value="1.25" />
			-->

			<!-- general balancing factor -->
			<DEV_AD_BALANCING_FACTOR value="1.0" />

			<!-- TRUE/1   FALSE/0 -->
			<DEV_ROUND_TO_BEAUTIFUL_VALUES value="TRUE" />

			<!-- example:
				"LOAD"-values:
				"*" -> load all
				"internalTest, myTest" -> load only "internalTest" and "myTest" creations

				"SKIP"-values:
				"*" -> skip all
				"internalTest" -> skip data by "internalTest"
			 -->

			<DEV_DATABASE_SKIP_ADS_CREATED_BY value="" />
			<DEV_DATABASE_LOAD_ADS_CREATED_BY value="*" />
			<DEV_DATABASE_SKIP_PROGRAMMES_CREATED_BY value="" />
			<DEV_DATABASE_LOAD_PROGRAMMES_CREATED_BY value="*" />

			<!--
			<DEV_STARTPROGRAMME_AD1_GUID value="speedminister-ad-spackeroog-01" />			
			<DEV_STARTPROGRAMME_AD2_GUID value="speedminister-ad-spargelsund-01" />			
			<DEV_STARTPROGRAMME_AD3_GUID value="speedminister-ad-rechtschreibdudan-01" />
			-->

			<!-- shortcuts DEV_NEWS_GUID1 - DEV_NEWS_GUID100 possible -->
			<DEV_NEWS_GUID1 value="ronny-news-sandsturm-01" />
			<DEV_NEWS_GUID2 value="ronny-news-usbratte-01" />


			<!-- use another AI script for player #2 -->
			<!--
			<PLAYERAISCRIPT2 value="res/ai/Sample/Sample.lua" />
			-->
		</data>
	</resources>
</tvt>
