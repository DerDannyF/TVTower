<?xml version="1.0" encoding="utf-8"?>
<tvt>
	<resources>
		<data name="DEV_CONFIG">
			<DEV_KEYS value="TRUE" />
			<DEV_OSD value="FALSE" />
			<DEV_LOG value="TRUE" />
			<!-- a modifier cutting the used factor to calculate the
			     revenue of a infomercial: 0.5 would cut the result to 50%
			     Default: 1.0
				<DEV_INFOMERCIALCUTFACTOR value="1.0" />
			-->

			<!-- milliseconds waiting on a floor (incl. animation).
			     Default: 1500
				<DEV_ELEVATOR_WAITTIME value="1500" />
			-->
			<!-- millseconds/frame (open/close animation, 8 frames each).
			     Default: 60
				<DEV_ELEVATOR_ANIMSPEED value="60" />
			-->
			<!-- speed in pixels per second.
			     Default: 130
				<DEV_ELEVATOR_SPEED value="140" />
			-->

			<DEV_DAILY_BOSS_VISIT value="1" />

			<!-- Which kind of contract sort is used in the adagency
			     Options:
			       "minaudience" - by minimum audience required
			       "classification" - by channel-groups (worst, avg, best) and daytime/primetime
			       "profit" - by profit of the contract
			     Default: "minaudience"
				<DEV_ADAGENCY_SORT_CONTRACTS_BY value="minaudience" />
			-->


			<!-- profit/penalty get multiplied with this value if there
			     is a limiting targetgroup.
			     Default: 3.0
				<DEV_AD_LIMITED_TARGETGROUP_MULTIPLIER value="3.0" />
			-->
			<!-- profit/penalty get multiplied with this value if there
			     is a limiting genre defined.
			     Default: 2.0
				<DEV_AD_LIMITED_GENRE_MULTIPLIER value="2.0" />
			-->

			<!-- general balancing factor -->
			<DEV_AD_BALANCING_FACTOR value="1.0" />

			<!-- TRUE/1   FALSE/0 -->
			<DEV_ROUND_TO_BEAUTIFUL_VALUES value="TRUE" />

			<!-- example:
				"LOAD"-values:
				"*" -> load all
				"internalTest, myTest" -> load only "internalTest" and "myTest" creations

				"SKIP"-values:
				"*" -> skip all
				"internalTest" -> skip data by "internalTest"
			 -->

			<DEV_DATABASE_SKIP_ADS_CREATED_BY value="" />
			<DEV_DATABASE_LOAD_ADS_CREATED_BY value="*" />
			<DEV_DATABASE_SKIP_PROGRAMMES_CREATED_BY value="" />
			<DEV_DATABASE_LOAD_PROGRAMMES_CREATED_BY value="*" />
			
		</data>
	</resources>
</tvt>
